            "errorMsg": "Field is required",
            "required": true,
            "options": [
              {
                "label": "Armenia",
                "value": "armenia"
              },
              {
                "label": "Russia",
                "value": "russia"
              }
            ]
          },
          {
            "id": "passportSerialNumber",
            "type": "text",
            "name": "passportSerialNumber",
            "title": "Passport serial number",
            "className": "textInput",
            "required": true,
            "errorMsg": "Field is required",
