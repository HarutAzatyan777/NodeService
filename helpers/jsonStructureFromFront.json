            "options": [
              {
                "title": "Yerevan",
                "value": "Yerevan"
              },
              {
                "title": "Gyumri",
                "value": "Gyumri"
              }
            ]
          },
          {
            "id": "address",
            "type": "text",
            "name": "address",
            "title": "Actual address in Armenia",
            "className": "input",
            "required": true,
            "errorMsg": "Field is required"
          },
          {
            "id": "maritalStatus",
            "type": "radio",
            "name": "maritalStatus",
            "title": "Are you married?",
            "required": true,
            "styleType": "type-1",
            "className": "radioInput",
            "errorMsg": "Please select reason",
            "options": [
              {
                "title": "Yes",
                "value": "yes"
              },
              {
                "title": "No",
                "value": "no"
              }
            ]
          }
        ]
      },
      {
        "stepNumber": 2,
        "name": "Basis for document submission",
