            "className": "input",
            "required": true
          },
          {
            "id": "nationality",
            "type": "dropdown",
            "name": "nationality",
            "title": "Nationality",
            "className": "input select-input",
            "errorMsg": "Field is required",
            "required": true,
            "options": [
              {
                "title": "Armenian",
                "value": "armenian"
              },
              {
                "title": "Russian",
                "value": "russian"
              }
            ]
          },
          {
            "id": "actualAddress",
            "type": "title",
            "title": "Actual address in Armenia"
          },
          {
            "id": "residenceCity",
            "type": "dropdown",
            "name": "residenceCity",
