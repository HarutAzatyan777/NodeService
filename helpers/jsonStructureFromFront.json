        "inputs": [
          {
            "id": "personalInformationTitle",
            "type": "title",
            "title": "Personal information"
          },
          {
            "id": "firstName",
            "type": "text",
            "name": "firstName",
            "title": "First Name",
            "className": "input",
            "required": true,
            "errorMsg": "First Name cannot contain numbers",
            "regex": "/^[a-zA-Z]+$/"
          },
          {
            "id": "lastName",
            "type": "text",
            "name": "lastName",
            "title": "Last Name",
            "className": "input",
            "required": true,
            "errorMsg": "Last Name cannot contain numbers",
            "regex": "/^[a-zA-Z]+$/"
          },
          {
            "id": "middleName",
            "type": "text",
            "name": "middleName",
            "title": "Middle Name",
            "className": "input",
            "required": true,
            "errorMsg": "Middle Name cannot contain numbers",
            "regex": "/^[a-zA-Z]+$/"
          },
          {
            "id": "birthCountryTitle",
            "type": "title",
            "title": "Place of birth"
          },
          {
            "id": "birthCountry",
            "type": "dropdown",
            "name": "birthCountry",
            "title": "birthCountry",
            "className": "input select-input",
            "errorMsg": "Field is required",
            "required": true,
            "options": [
              {
                "label": "Armenia",
                "value": "armenia"
              },
              {
                "label": "Russia",
                "value": "russia"
              }
            ]
          },
          {
            "id": "birthDate",
            "title": "Birth date",
            "name": "birthDate",
            "type": "date",
            "required": true,
            "className": "text"
