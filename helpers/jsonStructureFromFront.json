          {
            "id": "passportIssueDate",
            "title": "Date of issue",
            "name": "passportIssueDate",
            "type": "date",
            "required": true,
            "className": "text"
          },
          {
            "id": "passportIssuingAuthority",
            "type": "text",
            "name": "passportIssuingAuthority",
